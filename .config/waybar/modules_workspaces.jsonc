            "class<io.missioncenter.MissionCenter>": " ",
            "class<mpv>": " ",
            "class<code-oss>": "󰨞 ",
            "class<codium|codium-url-handler|VSCodium>": "󰨞 ",
            "class<mousepad>": " ",
            "class<org-jdownloader-update-launcher-JDLauncher>": " "
        },
